    "## üèóÔ∏è Portfolio Construction"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "id": "portfolio-setup"
   },
   "outputs": [],
   "source": [
    "# Equal-weighted portfolio\n",
    "weights = np.array([1/len(TICKERS)] * len(TICKERS))\n",
    "portfolio_returns = (returns * weights).sum(axis=1)\n",
    "portfolio_values = PORTFOLIO_VALUE * (1 + portfolio_returns).cumprod()\n",
    "\n",
    "# Portfolio statistics\n",
    "annual_return = portfolio_returns.mean() * 252\n",
    "annual_vol = portfolio_returns.std() * np.sqrt(252)\n",
    "sharpe_ratio = annual_return / annual_vol\n",
    "\n",
    "# Maximum drawdown\n",
    "cumulative = (1 + portfolio_returns).cumprod()\n",
    "running_max = cumulative.expanding().max()\n",
    "drawdown = (cumulative - running_max) / running_max\n",
    "max_drawdown = drawdown.min()\n",
    "\n",
    "print(\"üèóÔ∏è Portfolio Construction Complete!\")\n",
    "print(f\"\\nüìä Portfolio Statistics:\")\n",
    "print(f\"   ‚Ä¢ Equal Weight: {weights[0]:.1%} per asset\")\n",
    "print(f\"   ‚Ä¢ Annual Return: {annual_return:.2%}\")\n",
    "print(f\"   ‚Ä¢ Annual Volatility: {annual_vol:.2%}\")\n",
    "print(f\"   ‚Ä¢ Sharpe Ratio: {sharpe_ratio:.3f}\")\n",
    "print(f\"   ‚Ä¢ Max Drawdown: {max_drawdown:.2%}\")\n",
    "print(f\"   ‚Ä¢ Final Value: ${portfolio_values.iloc[-1]:,.2f}\")"
   ]
