    "def run_stress_test(base_returns, weights, scenario_name, modifications):\n",
    "    \"\"\"Run a stress test scenario.\"\"\"\n",
    "    \n",
    "    stressed_returns = base_returns.copy()\n",
    "    \n",
    "    # Apply modifications\n",
    "    if 'vol_multiplier' in modifications:\n",
    "        for col in stressed_returns.columns:\n",
    "            mean_ret = stressed_returns[col].mean()\n",
    "            stressed_returns[col] = mean_ret + (stressed_returns[col] - mean_ret) * modifications['vol_multiplier']\n",
    "    \n",
    "    if 'return_shock' in modifications:\n",
    "        for col in stressed_returns.columns:\n",
    "            stressed_returns[col] += modifications['return_shock']\n",
    "    \n",
    "    if 'asset_shocks' in modifications:\n",
    "        for asset, shock in modifications['asset_shocks'].items():\n",
    "            if asset in stressed_returns.columns:\n",
    "                stressed_returns[asset] += shock\n",
    "    \n",
    "    # Calculate stressed portfolio returns\n",
    "    stressed_portfolio = (stressed_returns * weights).sum(axis=1)\n",
    "    \n",
    "    # Run mini Monte Carlo\n",
    "    mc_stress = MonteCarloEngine(stressed_returns, weights)\n",
    "    stress_sims = mc_stress.simulate_portfolio_returns(10000)  # Smaller for speed\n",
    "    stress_metrics, stress_pnl = mc_stress.calculate_risk_metrics(stress_sims, PORTFOLIO_VALUE)\n",
    "    \n",
    "    return {\n",
    "        'scenario': scenario_name,\n",
    "        'var_95': stress_metrics['var_95'],\n",
    "        'var_99': stress_metrics['var_99'],\n",
    "        'cvar_95': stress_metrics['cvar_95'],\n",
    "        'cvar_99': stress_metrics['cvar_99'],\n",
    "        'annual_vol': stress_metrics['std_return'] * np.sqrt(252),\n",
    "        'portfolio_returns': stressed_portfolio\n",
    "    }\n",
    "\n",
    "# Define stress scenarios\n",
    "stress_scenarios = {\n",
    "    'Market Crash': {\n",
    "        'return_shock': -0.05,\n",
    "        'vol_multiplier': 1.5\n",
    "    },\n",
    "    'High Volatility': {\n",
    "        'vol_multiplier': 2.0\n",
    "    },\n",
    "    'Flight to Quality': {\n",
    "        'asset_shocks': {\n",
    "            'SPY': -0.03, 'EFA': -0.04, 'EEM': -0.06,\n",
    "            'TLT': 0.02, 'LQD': 0.01, 'GLD': 0.01, 'VNQ': -0.03\n",
    "        }\n",
    "    },\n",
    "    'Inflation Surge': {\n",
    "        'asset_shocks': {\n",
    "            'SPY': -0.02, 'EFA': -0.02, 'EEM': -0.01,\n",
    "            'TLT': -0.05, 'LQD': -0.03, 'GLD': 0.03, 'VNQ': 0.01\n",
    "        }\n",
    "    }\n",
    "}\n",
    "\n",
    "print(\"üî• Running Stress Tests...\")\n",
    "print(\"=\" * 50)\n",
    "\n",
    "stress_results = {}\n",
    "for scenario_name, modifications in stress_scenarios.items():\n",
    "    print(f\"\\nüìç {scenario_name}:\")\n",
    "    result = run_stress_test(returns, weights, scenario_name, modifications)\n",
    "    stress_results[scenario_name] = result\n",
    "    \n",
    "    print(f\"   ‚Ä¢ 95% VaR: ${result['var_95']:,.2f} (vs baseline ${risk_metrics['var_95']:,.2f})\")\n",
    "    print(f\"   ‚Ä¢ 99% VaR: ${result['var_99']:,.2f} (vs baseline ${risk_metrics['var_99']:,.2f})\")\n",
    "    print(f\"   ‚Ä¢ Annual Vol: {result['annual_vol']:.2%} (vs baseline {annual_vol:.2%})\")\n",
    "    \n",
    "    # Calculate change vs baseline\n",
    "    var_change = (result['var_95'] - risk_metrics['var_95']) / abs(risk_metrics['var_95']) * 100\n",
    "    print(f\"   ‚Ä¢ VaR Change: {var_change:+.1f}%\")\n",
    "\n",
    "print(\"\\n‚úÖ Stress testing complete!\")"
