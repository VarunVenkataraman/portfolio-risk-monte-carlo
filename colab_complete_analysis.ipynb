    "               name='Assets'),\n",
    "    row=2, col=1\n",
    ")\n",
    "\n",
    "# Add portfolio point\n",
    "fig.add_trace(\n",
    "    go.Scatter(x=[annual_vol*100], y=[annual_return*100], \n",
    "               mode='markers', marker=dict(size=15, color='red', symbol='star'),\n",
    "               name='Portfolio'),\n",
    "    row=2, col=1\n",
    ")\n",
